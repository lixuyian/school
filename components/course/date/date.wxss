/* components/course/date/date.wxss */
.warpper{
    display: grid;
    grid-template-rows: repeat(12,60px);
    font-size: 35rpx;
    align-content:center;
    justify-content: center;
    background: rgba(147, 209, 172, 0.3);
}
.warpper>view{
    border-right: 1px solid #ccc;
    border-bottom: 1px solid #ccc;
    padding: 0 10px;
    display: grid;
    align-content: center;
}
