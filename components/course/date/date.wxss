/* components/course/date/date.wxss */
.warpper{
    display: grid;
    grid-template-rows: repeat(12,50px);
    font-size: 35rpx;
    align-content:center;
    justify-content: center;
}
.warpper>view{
    background: rgba(147, 209, 172, 0.3);
    padding: 15rpx;
    border-right: 1px solid #ccc;
    border-bottom: 1px solid #ccc;
}